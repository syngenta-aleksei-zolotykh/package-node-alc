{
    "name": "alc-example",
    "version": "1.0.0",
    "description": "Just an example",
    "scripts": {
        "start": "serverless offline start"
    },
    "license": "ISC",
    "dependencies": {
        "@syngenta-digital/alc": "1.0.2"
    },
    "devDependencies": {
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.1.2",
        "serverless": "^2.6.0",
        "serverless-offline": "^6.8.0"
    },
    "engines": {
        "node": ">=12.14.1"
    },
    "eslintConfig": {
        "env": {
            "es6": true
        },
        "parserOptions": {
            "ecmaVersion": 9
        },
        "plugins": [
            "prettier"
        ],
        "extends": [
            "plugin:prettier/recommended"
        ],
        "rules": {
            "prettier/prettier": [
                "warn",
                {
                    "trailingComma": "none",
                    "printWidth": 120,
                    "tabWidth": 4,
                    "singleQuote": true,
                    "bracketSpacing": false,
                    "arrowParens": "always"
                }
            ],
            "eqeqeq": "error",
            "prefer-destucturing": 0,
            "no-global-assign": 0,
            "rest-spread-spacing": 1,
            "no-restricted-syntax": 0,
            "consistent-return": 0,
            "import/no-dynamic-require": 0,
            "no-param-reassign": 0,
            "global-require": 0,
            "class-methods-use-this": 0,
            "no-underscore-dangle": 0,
            "camelcase": 0
        }
    }
}
